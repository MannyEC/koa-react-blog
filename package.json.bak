{
  "name": "eckid-blog",
  "version": "2.3.0",
  "description": "React + Redux + react-router + antd",
  "license": "MIT",
  "repository": {
    "type": "",
    "url": ""
  },
  "main": "main.js",
  "sideEffects": false,
  "engines": {
    "node": ">=9.x",
    "npm": "^5.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "api": "better-npm-run api",
    "portal": "better-npm-run portal",
    "test": "jest --config jest.config.js",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "i18n": "better-npm-run i18n"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "i18n": {
      "command": "babel-node bin/i18n",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "node --max_old_space_size=2048 bin/server",
      "env": {
        "NODE_ENV": "development",
        "APP": "main",
        "DEBUG": "app:*",
        "PROTOCOL": "http",
        "API_HOST": "127.0.0.1",
        "API_PORT": 8910,
        "HOST": "localhost",
        "PORT": 4000,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      }
    },
    "api": {
      "command": "node --max_old_space_size=2048 bin/apiserver",
      "env": {
        "NODE_ENV": "development",
        "APP": "main",
        "DEBUG": "app:*",
        "PROTOCOL": "https",
        "API_HOST": "localhost",
        "API_PORT": 8900,
        "HOST": "localhost",
        "PORT": 4000,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      }
    },
    "portal": {
      "command": "node --max_old_space_size=2048 bin/server",
      "env": {
        "NODE_ENV": "development",
        "APP": "portal",
        "DEBUG": "app:*",
        "PROTOCOL": "https",
        "API_HOST": "1.1.1.1",
        "API_PORT": 443,
        "HOST": "localhost",
        "PORT": 5000,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "ant-design-pro": "^2.1.1",
    "antd": "^3.11.4",
    "classnames": "^2.2.5",
    "connected-react-router": "^6.0.0",
    "echarts": "^4.2.0-rc.2",
    "highcharts": "^7.0.1",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.5",
    "immutable": "^4.0.0-rc.12",
    "invariant": "^2.2.3",
    "ip": "^1.1.5",
    "keymirror": "^0.1.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "platform": "^1.3.5",
    "prop-types": "^15.6.1",
    "rc-queue-anim": "^1.4.1",
    "re-resizable": "^4.9.0",
    "react": "^16.7.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dom": "^16.7.0",
    "react-grid-layout": "^0.16.5",
    "react-highcharts": "^16.0.2",
    "react-hot-loader": "^4.6.3",
    "react-markdown": "^4.0.8",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.2.2",
    "react-select": "^2.1.2",
    "react-sizeme": "^2.3.6",
    "redux": "^4.0.1",
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
