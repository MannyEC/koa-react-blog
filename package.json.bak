    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "worker-loader": "^2.0.0",
    "yargs": "^12.0.5"
  }
}
