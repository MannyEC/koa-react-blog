{
  "name": "eckid-blog",
  "version": "1.0.0",
  "main": "main.js",
  "sideEffects": false,
  "engines": {
    "node": ">=9.x",
    "npm": ">=5.X"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "dev": "better-npm-run dev",
    "api": "better-npm-run api",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "package:dev": "better-npm-run package:dev",
    "package:prod": "better-npm-run package:prod"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "node --max_old_space_size=2048 bin/server",
      "env": {
        "NODE_ENV": "development",
        "APP": "main",
        "DEBUG": "app:*",
        "PROTOCOL": "http",
        "API_HOST": "127.0.0.1",
        "API_PORT": 8910,
        "HOST": "localhost",
        "PORT": 4000,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      }
    },
    "api": {
      "command": "node --max_old_space_size=2048 bin/apiserver",
      "env": {
        "NODE_ENV": "development",
        "APP": "main",
        "DEBUG": "app:*",
        "PROTOCOL": "http",
        "API_HOST": "localhost",
        "API_PORT": 8910,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      }
    },
    "package:dev": {
      "command": "node --max_old_space_size=2048 bin/server",
      "env": {
        "NODE_ENV": "development",
        "APP": "package",
        "DEBUG": "app:*",
        "PROTOCOL": "http",
        "API_HOST": "1.1.1.1",
        "API_PORT": 80,
        "HOST": "localhost",
        "PORT": 5000,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      }
    },
    "package:prod": {
      "command": "node bin/makePackage",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "antd": "^3.3.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "echarts": "^4.1.0",
    "echarts-gl": "^1.1.1",
    "fs": "0.0.1-security",
