  "version": "2.3.0",
  "description": "React + Redux + react-router + antd",
  "license": "MIT",
  "repository": {
    "type": "",
    "url": ""
  },
  "main": "main.js",
  "sideEffects": false,
  "engines": {
    "node": ">=9.x",
    "npm": "^5.0.0"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "dev:start": "better-npm-run dev:start",
    "build:server": "better-npm-run build:server",
    "build:client": "better-npm-run build:client",
    "just-server": "better-npm-run just-server"
  },
  "betterScripts": {
    "dev": {
      "command": "npm-run-all --parallel dev:** build:**",
      "env": {
        "NODE_ENV": "development",
        "APP": "main",
        "DEBUG": "app:*",
        "PROTOCOL": "http",
        "API_HOST": "127.0.0.1",
        "API_PORT": 8910,
        "HOST": "localhost",
        "PORT": 4000,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      }
    },
    "just-server": {
      "command": "npm-run-all --parallel dev:start build:server",
      "env": {
        "NODE_ENV": "development",
        "APP": "main",
        "DEBUG": "app:*",
        "PROTOCOL": "http",
        "API_HOST": "127.0.0.1",
        "API_PORT": 8910,
        "HOST": "localhost",
        "PORT": 4000,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      }
    },
    "dev:start": {
      "command": "nodemon --watch ektp_site --exec node \"./ektp_site/bundle.js\"",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "build:server": {
      "command": "webpack --config ./ektp_configs/webpack.config.server.js --watch",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "build:client": {
      "command": "webpack --config ./ektp_configs/webpack.config.client.js --watch",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
